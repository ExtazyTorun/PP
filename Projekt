struct hand
{
	int karta;

};

void createhand(struct hand tab[],int n,int player[5])
{
    system("cls");
    int i;
    for(i=0; i<n; i++){
        tab[i].karta[0]=player[i]
}
}

	








int main()
{	
    struct hand tab[5]; 
    int n=5;
	int player[5]; 
	int los;
	int karty[52]= {'1','2','3','4',
					'5','6','7','8',
					'9','a','q','w',
					'e','r','t','y',
					'u','i','o','p',
					'a','s','d','f',
					'g','h','j','k',
					'l','z','x','c',
					'v','b','n','m',
					'[',']','|','<',
					'>',',','.','?',
					'/','!','@','#',
					'$','%','^','*',};
	srand(time(NULL));
	

	
	for (int i=0; i<5; i++)
	{
			los = rand()%52;
			player[i] = karty[los];
	//		printf("%c\n", player[i]);
	}//losowanie 5 kart

	
	przypisanie(player);
	
	
}


