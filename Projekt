#include <stdlib.h>
#include <stdio.h>
#include <time.h>
#include <string.h>
void przypisanie(int player[5]) //przypisanie do wartosci konkretnych kart
{	
	printf("Twoje karty:");
	for (int i=0; i<5; i++)
	{
	if (player[i]=='1') printf("\nAs serce");
	if (player[i]=='2') printf("\nAs wino");
	if (player[i]=='3')printf("\nAs karo");
	if (player[i]=='4')printf("\nAs trefl");
	if (player[i]=='5')printf("\nKr贸l serce");
	if (player[i]=='6')printf("\nKr贸l wino");
	if (player[i]=='7')printf("\nKr贸l karo");
	if (player[i]=='8')printf("\nKr贸l trefl");
	if (player[i]=='9')printf("\nDama serce");
	if (player[i]=='q')printf("\nDama karo");
	if (player[i]=='w')printf("\nDama wino");
	if (player[i]=='e')printf("\nDama trefl");
	if (player[i]=='r')printf("\nWalet serce");
	if (player[i]=='t')printf("\nWalec wino");
	if (player[i]=='y')printf("\nWalet karo");
	if (player[i]=='u')printf("\nWalet trefl");
	if (player[i]=='i')printf("\n10 serce");
	if (player[i]=='o')printf("\n10 wino");
	if (player[i]=='p')printf("\n10 karo");
	if (player[i]=='a')printf("\n10 trefl");
	if (player[i]=='s')printf("\n9 wino");
	if (player[i]=='d')printf("\n9 serce");
	if (player[i]=='f')printf("\n9 karo");
	if (player[i]=='g')printf("\n9 trefl");
	if (player[i]=='h')printf("\n8 wino");
	if (player[i]=='j')printf("\n8 serce");
	if (player[i]=='k')printf("\n8 karo");
	if (player[i]=='l')printf("\n8 trefl");
	if (player[i]=='z')printf("\n7 wino");
	if (player[i]=='x')printf("\n7 serce");
	if (player[i]=='c')printf("\n7 karo");
	if (player[i]=='v')printf("\n7 trefl");
	if (player[i]=='b')printf("\n6 wino");
	if (player[i]=='n')printf("\n6 serce");
	if (player[i]=='m')printf("\n6 karo");
	if (player[i]=='[')printf("\n6 trefl");
	if (player[i]==']')printf("\n5 serce");
	if (player[i]=='|')printf("\n5 wino");
	if (player[i]=='<')printf("\n5 karo");
	if (player[i]=='>')printf("\n5 trefl");
	if (player[i]==',')printf("\n4 wino");
	if (player[i]=='.')printf("\n4 serce");
	if (player[i]=='?')printf("\n4 karo");
	if (player[i]=='/')printf("\n4 trefl");
	if (player[i]=='!')printf("\n3 wino");
	if (player[i]=='@')printf("\n3 serce");
	if (player[i]=='#')printf("\n3 karo");
	if (player[i]=='$')printf("\n3 trefl");
	if (player[i]=='%')printf("\n2 wino");
	if (player[i]=='^')printf("\n2 karo");
	if (player[i]=='!')printf("\n2 serce");
	if (player[i]=='*')printf("\n2 trefl");
	}
}
